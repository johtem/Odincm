{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.3.1",
      "destination": "wwwroot/lib/jquery-cdn",
      "files": [ "jquery.min.js" ]
    },
    {
      "library": "moment.js@2.22.2",
      "destination": "wwwroot/lib/moment-cdn",
      "files": [ "moment.min.js", "moment-with-locales.min.js" ]
    },
    {
      "library": "simplemde@1.11.2",
      "destination": "wwwroot/lib/simplemde-cdn",
      "files": [ "simplemde.min.css" ]
    },
    {
      "library": "twitter-bootstrap@4.1.3",
      "destination": "wwwroot/lib/bootstrap-cdn",
      "files": [ "css/bootstrap.css", "js/bootstrap.min.js", "css/bootstrap.min.css" ]
    },
    {
      "library": "popper.js@1.14.4",
      "destination": "wwwroot/lib/popperjs",
      "files": ["popper.min.js"]
    }
  ]
}